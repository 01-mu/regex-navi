{
  "is-main": true,
  "supported-targets": [
    "js"
  ],
  "import": [
    "mizchi/luna/luna/signal",
    {
      "path": "mizchi/luna/platform/dom/element",
      "alias": "dom"
    },
    {
      "path": "mizchi/js/browser/dom",
      "alias": "js_dom"
    },
    {
      "path": "internal/app/domain",
      "alias": "domain"
    },
    {
      "path": "internal/app/ui",
      "alias": "ui"
    }
  ],
  "link": {
    "js": {
      "format": "esm"
    }
  }
}
